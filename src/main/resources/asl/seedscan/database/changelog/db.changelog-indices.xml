    <createIndex clustered="true" indexName="tblchannel_pkchannelid_fksensorid_isIgnored_idx"
      tableName="tblchannel">
      <column name="pkchannelid"/>
      <column name="fksensorid"/>
      <column name="isIgnored"/>
